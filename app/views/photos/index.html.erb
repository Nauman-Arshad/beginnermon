<div class="px-1">
  <h1 class="text-4xl font-bold underline text-blue-700 font-mono text-center">Photos</h1>
  <%= link_to "New photo", new_photo_path, class: "bg-green-300 hover:bg-green-700 text-white py-2 px-4 rounded-md" %>
  <div id="photos">
    <% @photos.each do |photo| %>
      <%= render photo %>
    <% end %>
  </div>
</div>